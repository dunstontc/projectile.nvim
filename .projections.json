{
    "autoload/*.vim": {
        "type": "auto"
    },
    "doc/*.txt": {
        "type": "doc"
    },
    "plugin/*.vim": {
        "type": "plug"
    },
    "readme.md": {
        "type": "doc"
    },
    "rplugin/python3/denite/kind/*.py": {
        "alternate": "rplugin/python3/denite/source/{}.py",
        "type": "kind"
    },
    "rplugin/python3/denite/source/*.py": {
        "alternate": "rplugin/python3/denite/kind/{}.py",
        "type": "source"
    }
}
